<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>prueba</managed-bean-name>
		<managed-bean-class>mx.conavim.control.Prueba</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mbTbl_LineasAccion</managed-bean-name>
		<managed-bean-class>mx.conavim.servicios.MbTbl_LineasAccion</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>logout</managed-bean-name>
		<managed-bean-class>mx.conavim.servicios.Session</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>mbExportReport</managed-bean-name>
		<managed-bean-class>mx.conavim.servicios.MbExportReport</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>mbTbl_Informes</managed-bean-name>
		<managed-bean-class>mx.conavim.servicios.MbTbl_Informes</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	    <application>
	  <action-listener>
	    org.primefaces.application.DialogActionListener
	  </action-listener>
	  <navigation-handler>
	    org.primefaces.application.DialogNavigationHandler
	  </navigation-handler>
	  <view-handler>
	    org.primefaces.application.DialogViewHandler
	  </view-handler>
	</application>
</faces-config>
